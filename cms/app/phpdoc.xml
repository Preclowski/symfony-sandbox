<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Symfony Sandbox</title>
    <parser>
        <target>../../phpdoc</target>
        <extensions>
            <extension>php</extension>
        </extensions>
    </parser>
    <transformer>
        <target>../../phpdoc</target>
    </transformer>
    <transformations>
        <template name="responsive"/>
    </transformations>
    <logging>
        <level>warn</level>
        <paths>
            <default>../../test_result/phpdoc_log_{DATE}.log</default>
            <errors>../../test_result/phpdoc_log_{DATE}.errors.log</errors>
        </paths>
    </logging>
    <files>
        <directory>../src</directory>
        <ignore>App/**/Resources/*</ignore>
        <ignore>App/**/Tests/*</ignore>
        <ignore>App/**/Features/*</ignore>
        <ignore>App/DemoBundle/*</ignore>
    </files>
</phpdoc>
